programa
{
	
	funcao inicio()
	{
	inteiro a, b, troca
		escreva("Digite o valor numérico da variavel A: ")
		leia(a)
		escreva("Digite o valor numérico da variavel B: ")
		leia(b)

		troca = a
		a = b
		b = troca
		escreva("O novo valor de A é:", a)
		escreva("O novo valor de B é:", b)
	}
}
